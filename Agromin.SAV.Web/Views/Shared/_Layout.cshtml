<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - Soluciones Agromin</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="A fully featured admin theme which can be used to build CRM, CMS, etc." name="description" />
    <meta content="Coderthemes" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <link rel="shortcut icon" href="@Url.Content("~/Content/assets/images/favicon.ico")">
    <link href="@Url.Content("~/Content/assets/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/assets/css/icons.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/assets/css/app.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/assets/libs/custombox/custombox.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/assets/libs/flatpickr/flatpickr.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/assets/libs/bootstrap-colorpicker/bootstrap-colorpicker.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/assets/libs/clockpicker/bootstrap-clockpicker.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/assets/libs/sweetalert2/sweetalert2.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/assets/libs/feathericon/css/feathericon.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/css/select2.css")" rel="stylesheet" />
    @RenderSection("styles", required: false)
</head>

<body>
    <style>
        .content-page {
            padding: 20px;
            left: 20px !important;
            right: 20px !important;
            background -position: right bottom;
                        background-repeat: no-repeat;
         }
    </style>
    <div id="wrapper">

        @{ Html.RenderPartial("_Topbar");}
        @{ Html.RenderPartial("_LeftSidebar");}
        <div class="content-page">
            <div class="content">
                <div class="container-fluid">
                    @Html.Alert(TempData)
                    <div class="row">
                        <div class="col-12">
                            <div class="page-title-box">
                                <div class="page-title-right">
                                    <ol class="breadcrumb m-0">
                                        <li class="breadcrumb-item"><a href="javascript: void(0);">Soluciones Agromin</a></li>
                                        <li class="breadcrumb-item"><a href="javascript: void(0);">@ViewBag.Section</a></li>
                                        <li class="breadcrumb-item active">@ViewBag.Title</li>
                                    </ol>
                                </div>
                                <h4 class="page-title"><i class="@ViewBag.Icon"></i> @ViewBag.Title</h4>
                            </div>
                        </div>
                    </div>
                    @RenderBody()
                </div>
                @{ Html.RenderPartial("_Footer");}
            </div>
        </div>
        @{ Html.RenderPartial("_RightSidebar");}
        <div class="rightbar-overlay"></div>
        <script src="@Url.Content("~/Content/assets/js/vendor.min.js")"></script>
        <script src="@Url.Content("~/Content/assets/js/app.min.js")"></script>
        <script src="@Url.Content("~/Content/assets/libs/custombox/custombox.min.js")"></script>
        <script src="@Url.Content("~/Content/assets/libs/flatpickr/flatpickr.min.js")"></script>
        <script src="@Url.Content("~/Content/assets/libs/bootstrap-colorpicker/bootstrap-colorpicker.min.js")"></script>
        <script src="@Url.Content("~/Content/assets/libs/clockpicker/bootstrap-clockpicker.min.js")"></script>
        <script src="@Url.Content("~/Content/assets/js/pages/form-pickers.init.js")"></script>
        <script src="@Url.Content("~/Content/assets/libs/sweetalert2/sweetalert2.min.js")"></script>
        <script src="@Url.Content("~/Content/assets/libs/feathericon/tests/index.js")"></script>
        <script src="~/Scripts/select2.js"></script>
        <script src="@Url.Content("~/Scripts/Pages/Helpers/JsInputValidationHelpers.js")"></script>


        @Html.Content("/Shared/global.min.js", true)
        @RenderSection("scripts", required: false)
        <script src="@Url.Content("~/Scripts/shared/GlobalHelps.js")"></script>
        <script>
        </script>

</body>
</html>



